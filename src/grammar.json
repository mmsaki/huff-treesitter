    "interface_primitives": {
      "type": "FIELD",
      "name": "type",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(address|string\\d*|bytes\\d*|int\\d*|uint\\d*|bool|hash\\d*)"
        }
      }
    },
    "interface_extensions": {
      "type": "FIELD",
      "name": "modifier",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(nonpayable|view|returns)"
        }
      }
    },
    "predeclaration_template": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "keyword",
          "content": {
            "type": "STRING",
            "value": "template"
          }
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "FIELD",
          "name": "parameters",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "SYMBOL",
                      "name": "identifier"
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "opcodes": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opcodes_stop"
        },
        {
          "type": "SYMBOL",
          "name": "opcodes_calculation"
        },
        {
          "type": "SYMBOL",
          "name": "opcodes_stack"
        },
        {
          "type": "SYMBOL",
          "name": "opcodes_io"
        },
        {
          "type": "SYMBOL",
          "name": "template_parameter_call"
        }
      ]
    },
    "opcodes_io": {
      "type": "FIELD",
      "name": "opcode",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(sstore|sload|mstore8|mstore|mload|pop|msize|balance|address|returndatacopy|returndatasize|extcodecopy|extcodesize|gasprice|caller|origin|gaslimit|difficulty|number|timestamp|coinbase|blockhash|codecopy|codesize|calldatacopy|calldatasize|calldataload|callvalue|gas)"
        }
      }
    },
    "opcodes_side_effects": {
      "type": "FIELD",
      "name": "opcode",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(log4|log3|log2|log1|log0|jumpdest|getpc|jumpi|jump|create2|staticcall|delegatecall|callcode|call|create)"
        }
      }
    },
    "opcodes_calculation": {
      "type": "FIELD",
      "name": "opcode",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(not|xor|or|and|ror|rol|sar|shr|shl|keccak|sha3|byte|iszero|eq|sgt|slt|gt|lt|signextend|exp|mulmod|addmod|smod|mod|sdiv|div|sub|mul|add)"
        }
      }
    },
    "opcodes_stop": {
      "type": "FIELD",
      "name": "opcode",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "(selfdestruct|invalid|revert|return|stop)"
        }
      }
    },
    "opcodes_stack": {
      "type": "FIELD",
      "name": "opcode",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "((swap1|dup1)[0-6]|(swap|dup)[1-9]|push3[0-2]|push[1-2][0-9]|push[0-9])"
        }
      }
    },
    "template_parameter_call": {
      "type": "FIELD",
      "name": "parameter",
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "PATTERN",
          "value": "<\\s*[A-Za-z0-9_]+\\s*>"
        }
      }
    },
    "macro_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
