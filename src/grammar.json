{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "huff",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_patterns"
      }
    },
    "_patterns": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natspec"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "declaration"
        },
        {
          "type": "SYMBOL",
          "name": "interface"
        },
        {
          "type": "SYMBOL",
          "name": "error"
        },
        {
          "type": "SYMBOL",
          "name": "control"
        },
        {
          "type": "SYMBOL",
          "name": "macro_call"
        },
        {
          "type": "SYMBOL",
          "name": "opcodes"
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        }
      ]
    },
    "natspec": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natspec_line"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_block"
        }
      ]
    },
    "natspec_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "///"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "natspec_tags"
              }
            ]
          }
        }
      ]
    },
    "natspec_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/**"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "natspec_tags"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "natspec_tags": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natspec_tag_title"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_tag_author"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_tag_notice"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_tag_dev"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_tag_param"
        },
        {
          "type": "SYMBOL",
          "name": "natspec_tag_return"
        }
      ]
    },
    "natspec_tag_title": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@title"
      }
    },
    "natspec_tag_author": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@author"
      }
    },
    "natspec_tag_notice": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@notice"
      }
    },
    "natspec_tag_dev": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@dev"
      }
    },
    "natspec_tag_param": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "keyword",
            "content": {
              "type": "STRING",
              "value": "@param"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "param_name",
                "content": {
                  "type": "SYMBOL",
                  "name": "identifier"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "natspec_tag_return": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "keyword",
            "content": {
              "type": "STRING",
              "value": "@return"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "param_name",
                "content": {
                  "type": "SYMBOL",
                  "name": "identifier"
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z_]\\w*"
    },
    "comment_line": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "comment_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^*]|(\\*[^/])"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_line"
        },
        {
          "type": "SYMBOL",
          "name": "comment_block"
        }
      ]
    },
    "control": {
      "type": "SYMBOL",
      "name": "control_import"
    },
    "control_import": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "#include"
      }
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number_decimal"
        },
        {
          "type": "SYMBOL",
          "name": "number_hex"
        }
      ]
    },
    "number_decimal": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\d+(\\.\\d+)?"
      }
    },
    "number_hex": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "0[xX][a-fA-F0-9]+"
      }
    },
    "declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "declaration_macro"
        },
        {
          "type": "SYMBOL",
          "name": "declaration_fn"
        },
        {
          "type": "SYMBOL",
          "name": "declaration_jumptable"
        },
        {
          "type": "SYMBOL",
          "name": "declaration_jumptable_packed"
        }
      ]
    },
    "declaration_macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "keyword",
          "content": {
            "type": "STRING",
            "value": "#define macro"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "FIELD",
                  "name": "takes_keyword",
                  "content": {
                    "type": "STRING",
                    "value": "takes"
                  }
                },
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "FIELD",
                  "name": "takes_count",
                  "content": {
                    "type": "SYMBOL",
                    "name": "number_decimal"
                  }
                },
                {
                  "type": "STRING",
                  "value": ")"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "returns_keyword",
                          "content": {
                            "type": "STRING",
                            "value": "returns"
                          }
                        },
                        {
                          "type": "STRING",
                          "value": "("
                        },
                        {
                          "type": "FIELD",
                          "name": "returns_count",
                          "content": {
                            "type": "SYMBOL",
                            "name": "number_decimal"
                          }
                        },
                        {
                          "type": "STRING",
                          "value": ")"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "declaration_fn": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "keyword",
          "content": {
            "type": "STRING",
            "value": "#define fn"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
