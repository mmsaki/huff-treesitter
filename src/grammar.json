                },
                {
                  "type": "FIELD",
                  "name": "takes_keyword",
                  "content": {
                    "type": "STRING",
                    "value": "takes"
                  }
                },
                {
                  "type": "STRING",
                  "value": "("
                },
                {
                  "type": "FIELD",
                  "name": "takes_count",
                  "content": {
                    "type": "SYMBOL",
                    "name": "number_decimal"
                  }
                },
                {
                  "type": "STRING",
                  "value": ")"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "returns_keyword",
                          "content": {
                            "type": "STRING",
                            "value": "returns"
                          }
                        },
                        {
                          "type": "STRING",
                          "value": "("
                        },
                        {
                          "type": "FIELD",
                          "name": "returns_count",
                          "content": {
                            "type": "SYMBOL",
                            "name": "number_decimal"
                          }
                        },
                        {
                          "type": "STRING",
                          "value": ")"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "declaration_jumptable": {
