    "interface_function": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "keyword",
          "content": {
            "type": "STRING",
            "value": "#define function"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "parameters",
          "content": {
            "type": "SYMBOL",
            "name": "parameter_list"
